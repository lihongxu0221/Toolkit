f}}" />        <Setter Property="bg:BackgroundSwitchElement.Background" Value="#586a80" />        <Setter Property="bg:BackgroundSwitchElement.MouseDownBackground"  Value="{DynamicResource MouseDownBrush}" />        <Setter Property="bg:BackgroundSwitchElement.MouseHoverBackground"  Value="{DynamicResource MouseHoverBrush}" />        <Setter Property="BorderBrush" Value="{DynamicResource BorderBrush}" />        <Setter Property="BorderThickness" Value="1" />        <Setter Property="Foreground" Value="{DynamicResource PrimaryTextBrush}" />        <Setter Property="Padding" Value="0 5 0 5" />        <Setter Property="RenderTransformOrigin" Value="0.5,0.5" />        <Setter Property="RenderTransform">            <Setter.Value>                <ScaleTransform x:Name="ImageScale" ScaleX="1" ScaleY="1" />            </Setter.Value>        </Setter>        <Setter Property="Template">            <Setter.Value>                <ControlTemplate TargetType="RadioButton">                    <bg:SimplePanel>                        <Border x:Name="PanelMain" Background="{TemplateBinding Background}" Opacity="0.6"                                CornerRadius="{Binding Path=(bg:BorderElement.CornerRadius), RelativeSource={RelativeSource TemplatedParent}}" />                        <Border BorderThickness="{TemplateBinding BorderThickness}"                                 BorderBrush="{TemplateBinding BorderBrush}"                                CornerRadius="{Binding Path=(bg:BorderElement.CornerRadius), RelativeSource={RelativeSource TemplatedParent}}">                            <Grid HorizontalAlignment="Center" VerticalAlignment="Center">                                <Grid.ColumnDefinitions>                                    <ColumnDefinition Width="Auto" />                                    <ColumnDefinition Width="*" />                                </Grid.ColumnDefinitions>                                <TextBlock Grid.Column="0"                                            HorizontalAlignment="Center" VerticalAlignment="Center"                                            FontFamily="{DynamicResource IconFont}"                                            Text="{Binding Icon}"                                           Margin="{Binding (bg:IconElement.Margin),RelativeSource={RelativeSource Mode=TemplatedParent}}"                                           FontSize="{Binding (bg:IconElement.FontSize),RelativeSource={RelativeSource Mode=TemplatedParent}}"                                           Foreground="{TemplateBinding Foreground}"/>                                <TextBlock Grid.Column="1"                                            HorizontalAlignment="Center" VerticalAlignment="Center"                                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"                                           Text="{Binding Name}"                                           Margin="{TemplateBinding Padding}"                                           FontSize="{TemplateBinding FontSize}"                                           Foreground="{TemplateBinding Foreground}"/>                            </Grid>                        </Border>                    </bg:SimplePanel>                    <ControlTemplate.Triggers>                        <Trigger Property="IsEnabled" Value="False">                            <Setter Property="Background" Value="{DynamicResource SecondaryRegionBrush}" />                            <Setter Property="Opacity" Value="0.6" />                        </Trigger>                        <Trigger Property="IsChecked" Value="True">                            <Setter Property="Foreground" Value="{DynamicResource PrimaryTextBrush}" />                            <Setter Property="BorderBrush" Value="{Binding Path=(bg:BackgroundSwitchElement.MouseDownBackground),RelativeSource={RelativeSource Self}}" />                            <Setter Property="Background" Value="{Binding Path=(bg:BackgroundSwitchElement.MouseDownBackground),RelativeSource={RelativeSource Self}}" />                        </Trigger>                        <Trigger Property="IsMouseOver" Value="True">                            <Setter Property="Foreground" Value="{DynamicResource PrimaryTextBrush}" />                            <Setter Property="BorderBrush" Value="{Binding Path=(bg:BackgroundSwitchElement.MouseHoverBackground),RelativeSource={RelativeSource Self}}" />                            <Setter Property="Background" Value="{Binding Path=(bg:BackgroundSwitchElement.MouseHoverBackground),RelativeSource={RelativeSource Self}}" />                        </Trigger>                        <Trigger Property="IsPressed" Value="True">                            <Setter Property="Foreground" Value="{DynamicResource PrimaryTextBrush}" />                            <Setter Property="BorderBrush" Value="{Binding Path=(bg:BackgroundSwitchElement.MouseDownBackground),RelativeSource={RelativeSource Self}}" />                            <Setter Property="Background" Value="{Binding Path=(bg:BackgroundSwitchElement.MouseDownBackground),RelativeSource={RelativeSource Self}}" />                        </Trigger>                    </ControlTemplate.Triggers>                </ControlTemplate>            </Setter.Value>        </Setter>    </Style>    <DataTemplate x:Key="ListBoxStyleTemplateMain" DataType="{x:Type bg:ModuleInfo}">        <RadioButton Style="{DynamicResource RadioButtonStyle.FontIconAboveText}"                      Content="{Binding DisplayName}"                     Width="Auto"                     Height="Auto"                     Margin="{DynamicResource MainTreeButtonMargin}"                     bg:BorderElement.CornerRadius="{DynamicResource MainTreeButtonCornerRadius}"                     FontSize ="{DynamicResource MainTreeButtonFontSize}"                     bg:IconElement.FontSize="{DynamicResource MainTreeButtonIconSize}"                     bg:IconElement.Margin="{DynamicResource MainTreeButtonIconMargin}"                     bg:BackgroundSwitchElement.Background="{StaticResource SecondaryRegionBrush}"                     Padding="{DynamicResource MainTreeButtonFontMargin}"                     BorderThickness="0"/>    </DataTemplate>    <DataTemplate x:Key="ListBoxStyleTemplateSubMain" DataType="{x:Type bg:ModuleInfo}">        <RadioButton Style="{DynamicResource RadioButtonStyle.FontIconBeforeText}"                      Content="{Binding DisplayName}"                     Width ="Auto"                     Height="{DynamicResource SubMainTreeButtonHeight}"                     Margin="{DynamicResource SubMainTreeButtonMargin}"                     bg:BorderElement.CornerRadius="{DynamicResource SubMainTreeButtonCornerRadius}"                     FontSize ="{DynamicResource SubMainTreeButtonFontSize}"                     bg:IconElement.FontSize="{DynamicResource SubMainTreeButtonIconSize}"                     bg:IconElement.Margin="{DynamicResource SubMainTreeButtonIconMargin}"                     bg:BackgroundSwitchElement.Background="{DynamicResource ThirdlyRegionBrush }"                     Padding="{DynamicResource SubMainTreeButtonFontMargin}"                     BorderBrush="{DynamicResource SecondaryBorderBrush}"/>    </DataTemplate>    <!--模块信息列表-->    <Style x:Key="ModuleViewsListBoxStyle" TargetType="ListBox" BasedOn="{StaticResource ListBoxCustom}">        <Setter Property="ItemsSource" Value="{Binding Views}" />        <Setter Property="SelectedItem" Value="{Binding SelectedView}"/>        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>        <Setter Property="VerticalContentAlignment" Value="Stretch"/>        <Setter Property="Background" Value="Transparent" />        <Setter Property="BorderBrush" Value="Transparent"/>        <Setter Property="BorderThickness" Value="0"/>        <Setter Property="SelectionMode" Value="Single"/>        <Setter Property="bg:BorderElement.CornerRadius" Value="10" />        <Setter Property="ItemTemplate" Value="{StaticResource ListBoxStyleTemplateMain}" />        <Setter Property="ItemsPanel">            <Setter.Value>                <ItemsPanelTemplate>                    <VirtualizingStackPanel Orientation="Vertical"/>                </ItemsPanelTemplate>            </Setter.Value>        </Setter>        <Setter Property="ItemContainerStyle">            <Setter.Value>                <Style TargetType="ListBoxItem">                    <Setter Property="IsSelected" Value="{Binding IsSelected,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True,NotifyOnTargetUpdated=True}" />                    <Setter Property="SnapsToDevicePixels" Value="True" />                    <Setter Property="FocusVisualStyle" Value="{x:Null}" />                    <Setter Property="Padding" Value="0" />                    <!--<Setter Property="HorizontalContentAlignment">                        <Setter.Value>                            <Binding Path="HorizontalContentAlignment" RelativeSource="{RelativeSource AncestorType=ItemsControl}" FallbackValue="Stretch" />                        </Setter.Value>                    </Setter>-->                    <Setter Property="HorizontalContentAlignment" Value="Stretch" />                    <Setter Property="VerticalContentAlignment" Value="Stretch" />                    <Setter Property="Background" Value="Transparent" />                    <Setter Property="BorderBrush" Value="Transparent" />                    <Setter Property="BorderThickness" Value="0" />                    <Setter Property="Template">                        <Setter.Value>                            <ControlTemplate TargetType="{x:Type ListBoxItem}">                                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"                                                   SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"                                                   VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />                            </ControlTemplate>                        </Setter.Value>                    </Setter>                </Style>            </Setter.Value>        </Setter>    </Style>    <SolidColorBrush x:Key="Window.NonClientAreaBackground" Color="{DynamicResource RegionColor}" />    <SolidColorBrush x:Key="CloseButtonBackground" Color="Transparent" />    <SolidColorBrush x:Key="CloseButtonForeground" Color="{DynamicResource PrimaryTextColor}" />    <SolidColorBrush x:Key="CloseButtonHoverBackground" Color="#e81123" />    <SolidColorBrush x:Key="CloseButtonHoverForeground" Color="{DynamicResource PrimaryTextColor}" />    <SolidColorBrush x:Key="OtherButtonBackground" Color="Transparent" />    <SolidColorBrush x:Key="OtherButtonForeground" Color="{DynamicResource PrimaryTextColor}" />    <SolidColorBrush x:Key="OtherButtonHoverBackground" Color="{DynamicResource ThirdlyRegionColor}" />    <SolidColorBrush x:Key="OtherButtonHoverForeground" Color="{DynamicResource PrimaryTextColor}" />    <Style x:Key="MainWindowStyle" TargetType="{x:Type Window}">        <Setter Property="WindowState" Value="Normal" />        <Setter Property="WindowStyle" Value="None"/>        <Setter Property="ResizeMode" Value="NoResize"/>        <!--<Setter Property="AllowsTransparency" Value="True"/>-->        <Setter Property="BorderThickness" Value="1" />        <Setter Property="HorizontalContentAlignment" Value="Center" />        <Setter Property="VerticalContentAlignment" Value="Center" />        <Setter Property="FrameworkElement.UseLayoutRounding" Value="True" />        <Setter Property="BorderBrush" Value="{DynamicResource BorderBrush}" />        <Setter Property="Background" Value="{DynamicResource SecondaryRegionBrush}" />        <Setter Property="Foreground" Value="{DynamicResource PrimaryTextBrush}" />        <Setter Property="FontFamily" Value="Microsoft YaHei UI" />        <Setter Property="FontSize" Value="12" />        <Setter Property="bg:BorderElement.CornerRadius" Value="0" />        <Setter Property="WindowChrome.WindowChrome">            <Setter.Value>                <WindowChrome CornerRadius="0" GlassFrameThickness="1" UseAeroCaptionButtons="False" CaptionHeight="0"/>            </Setter.Value>        </Setter>        <Setter Property="Template">            <Setter.Value>                <!--主界面窗体样式-->                <ControlTemplate TargetType="{x:Type Window}">                    <Border SnapsToDevicePixels="True"                             CornerRadius="{TemplateBinding bg:BorderElement.CornerRadius}"                            BorderThickness="{TemplateBinding BorderThickness}"                             BorderBrush="{TemplateBinding BorderBrush}"                             Background="{TemplateBinding Background}">                        <Grid Margin="{TemplateBinding Padding}" Background="{DynamicResource RegionBrush}">                            <ContentPresenter ClipToBounds="True" ContentSource="Content" />                            <Grid x:Name="PART_NonClientArea" HorizontalAlignment="Right" VerticalAlignment="Top" Height="{DynamicResource WindowTitleHeight}">                                <StackPanel Name="ButtonPanel" HorizontalAlignment="Right" Orientation="Horizontal" WindowChrome.IsHitTestVisibleInChrome="True">                                    <Button Name="ButtonMin"                                             Width="{DynamicResource WindowButttonWidth}"                                             Height="{DynamicResource WindowButttonHeight}"                                            Style="{DynamicResource Button.GeometryIcon}"                                            bg:IconElement.Geometry="{StaticResource WindowMinGeometry}"                                            bg:IconElement.Width="8"                                            bg:IconElement.Height="8"                                            Background="{StaticResource OtherButtonBackground}"                                            Foreground="{StaticResource OtherButtonForeground}"                                            Command="SystemCommands.MinimizeWindowCommand"                                            IsTabStop="False"                                            BorderThickness="1,0,1,0"/>                                    <Button Name="ButtonMax"                                             Width="{DynamicResource WindowButttonWidth}"                                             Height="{DynamicResource WindowButttonHeight}"                                            Style="{DynamicResource Button.GeometryIcon}"                                            bg:IconElement.Geometry="{DynamicResource WindowMaxGeometry}"                                            bg:IconElement.Width="8"                                            bg:IconElement.Height="8"                                            Background="{StaticResource OtherButtonBackground}"                                            Foreground="{StaticResource OtherButtonForeground}"                                            Command="SystemCommands.MaximizeWindowCommand"                                            IsTabStop="False"                                            BorderThickness="1,0,1,0" />                                    <Button Name="ButtonRestore"                                             Width="{DynamicResource WindowButttonWidth}"                                             Height="{DynamicResource WindowButttonHeight}"                                            Style="{DynamicResource Button.GeometryIcon}"                                            bg:IconElement.Geometry="{DynamicResource WindowRestoreGeometry}"                                            bg:IconElement.Width="8"                                            bg:IconElement.Height="8"                                            Background="{StaticResource OtherButtonBackground}"                                            Foreground="{StaticResource OtherButtonForeground}"                                            Command="SystemCommands.RestoreWindowCommand"                                            IsTabStop="False"                                            BorderThickness="1,0,1,0" />                                    <Button Name="ButtonClose"                                             Width="{DynamicResource WindowButttonWidth}"                                             Height="{DynamicResource WindowButttonHeight}"                                            Style="{DynamicResource Button.GeometryIcon}"                                            bg:IconElement.Geometry="{DynamicResource CloseGeometry}"                                            bg:IconElement.Width="8"                                            bg:IconElement.Height="8"                                            Background="{StaticResource CloseButtonBackground}"                                            Foreground="{StaticResource CloseButtonForeground}"                                            Command="SystemCommands.CloseWindowCommand"                                            IsTabStop="False"                                            BorderThickness="1,0,0,0"/>                                </StackPanel>                            </Grid>                            <ResizeGrip x:Name="ResizeGrip"                                         Width="10"                                         Height="10"                                         HorizontalAlignment="Right"                                        VerticalAlignment="Bottom"                                         IsTabStop="False"                                         Visibility="Collapsed"                                         WindowChrome.ResizeGripDirection="BottomRight" />                        </Grid>                    </Border>                    <ControlTemplate.Triggers>                        <Trigger Property="IsActive" Value="False">                            <Setter TargetName="ButtonPanel" Property="Opacity" Value=".8" />                        </Trigger>                        <Trigger Property="WindowState" Value="Maximized">                            <Setter TargetName="ButtonMax" Property="Visibility" Value="Collapsed" />                            <Setter TargetName="ButtonRestore" Property="Visibility" Value="Visible" />                        </Trigger>                        <Trigger Property="WindowState" Value="Normal">                            <Setter TargetName="ButtonMax" Property="Visibility" Value="Visible" />                            <Setter TargetName="ButtonRestore" Property="Visibility" Value="Collapsed" />                        </Trigger>                        <!--<Trigger Property="ResizeMode" Value="NoResize">                            <Setter TargetName="ButtonMin" Property="Visibility" Value="Collapsed" />                            <Setter TargetName="ButtonMax" Property="Visibility" Value="Collapsed" />                            <Setter TargetName="ButtonRestore" Property="Visibility" Value="Collapsed" />                        </Trigger>                        <Trigger Property="ResizeMode" Value="CanMinimize">                            <Setter TargetName="ButtonMax" Property="Visibility" Value="Collapsed" />                            <Setter TargetName="ButtonRestore" Property="Visibility" Value="Collapsed" />                        </Trigger>-->                        <Trigger SourceName="ButtonClose" Property="IsMouseOver" Value="True">                            <Setter TargetName="ButtonClose" Property="Background" Value="{StaticResource CloseButtonHoverBackground}" />                            <Setter TargetName="ButtonClose" Property="Foreground" Value="{StaticResource CloseButtonHoverForeground}" />                        </Trigger>                        <Trigger SourceName="ButtonMax" Property="IsMouseOver" Value="True">                            <Setter TargetName="ButtonMax" Property="Background" Value="{StaticResource OtherButtonHoverBackground}" />                            <Setter TargetName="ButtonMax" Property="Foreground" Value="{StaticResource OtherButtonHoverForeground}" />                        </Trigger>                        <Trigger SourceName="ButtonRestore" Property="IsMouseOver" Value="True">                            <Setter TargetName="ButtonRestore" Property="Background" Value="{StaticResource OtherButtonHoverBackground}" />                            <Setter TargetName="ButtonRestore" Property="Foreground" Value="{StaticResource OtherButtonHoverForeground}" />                        </Trigger>                        <Trigger SourceName="ButtonMin" Property="IsMouseOver" Value="True">                            <Setter TargetName="ButtonMin" Property="Background" Value="{StaticResource OtherButtonHoverBackground}" />                            <Setter TargetName="ButtonMin" Property="Foreground" Value="{StaticResource OtherButtonHoverForeground}" />                        </Trigger>                        <MultiTrigger>                            <MultiTrigger.Conditions>                                <Condition Property="ResizeMode" Value="CanResizeWithGrip" />                                <Condition Property="WindowState" Value="Normal" />                            </MultiTrigger.Conditions>                            <Setter TargetName="ResizeGrip" Property="Visibility" Value="Visible" />                        </MultiTrigger>                    </ControlTemplate.Triggers>                </ControlTemplate>            </Setter.Value>        </Setter>    </Style></ResourceDictionary>